<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>familyFinancial</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>


  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.LogoutServlet</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>reteriveUserTable</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.UserTableServlet</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>reteriveProfileTable</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.ProfileTableServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>reteriveCategoryTable</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.CategoryTableServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>reteriveItemTable</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.ItemTableServlet</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>userManagementServlet</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.UserManagementServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>profileManagementServlet</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.ProfileManagementServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>categoryManagementServlet</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.CategoryManagementServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>itemManagementServlet</servlet-name>
  	<servlet-class>com.hi5.ff.servlet.ItemManagementServlet</servlet-class>
  </servlet>

  <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

  <servlet-mapping>
        <servlet-name>reteriveUserTable</servlet-name>
        <url-pattern>/reteriveUserTable</url-pattern>
    </servlet-mapping>

   <servlet-mapping>
        <servlet-name>reteriveProfileTable</servlet-name>
        <url-pattern>/reteriveProfileTable</url-pattern>
    </servlet-mapping>

       <servlet-mapping>
        <servlet-name>reteriveCategoryTable</servlet-name>
        <url-pattern>/reteriveCategoryTable</url-pattern>
    </servlet-mapping>

     <servlet-mapping>
        <servlet-name>reteriveItemTable</servlet-name>
        <url-pattern>/reteriveItemTable</url-pattern>
    </servlet-mapping>

  <servlet-mapping>
        <servlet-name>userManagementServlet</servlet-name>
        <url-pattern>/userManagementServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>profileManagementServlet</servlet-name>
        <url-pattern>/profileManagementServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>categoryManagementServlet</servlet-name>
        <url-pattern>/categoryManagementServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>itemManagementServlet</servlet-name>
        <url-pattern>/itemManagementServlet</url-pattern>
    </servlet-mapping>

</web-app>